<form action="/register" method="post">

  <br><br>
  <label for="nick" >
    Enter your nick:
    <input type="text" name="nick" required></input>
  </label>

  <br>
  <label for="password">
    Enter your password:
    <input type="password" name="password" required></input>
  </label>

  <br>
  <label for="email">
    Enter your email:
    <input type="email" name="email" required></input>
  </label>

  <br>
  <button>submit</button>
</form>
